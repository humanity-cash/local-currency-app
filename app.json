{
  "name": "berkshares",
  "displayName": "BerkShares",
  "expo": {
    "name": "BerkShares",
    "slug": "berkshares",
    "version": "1.0.18",
    "icon": "./assets/images/icon_white.png",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#cbd5cf"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.keyko.berkshares-app",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "BerkShares uses the camera to scan barcodes or edit your profile.",
        "NSPhotoLibraryAddUsageDescription": "BerkShares uses the photo library to upload your profile.",
        "NSPhotoLibraryUsageDescription": "BerkShares uses the photo library to upload your profile.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx",
        "pfd",
        "ttf"
      ]
    },
    "android": {
      "package": "com.berkshares",
      "versionCode": 4,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS"
      ]
    }
  }
}